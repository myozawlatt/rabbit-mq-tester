    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationIcon>rabbitMQ.ico</ApplicationIcon>
    <Version>1.0</Version>
    <Authors>Myo Zaw Latt</Authors>    
  </PropertyGroup>

  <ItemGroup>
    <Content Include="rabbitMQ.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MassTransit" Version="8.3.1" />
